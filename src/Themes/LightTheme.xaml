<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush x:Key="App.Window.Background" Color="White"/>
    <SolidColorBrush x:Key="App.Border.Default" Color="#B0B0B0"/>
    <SolidColorBrush x:Key="App.Text.Default" Color="Black"/>
    <SolidColorBrush x:Key="App.Text.Grayed" Color="Gray"/>
    <SolidColorBrush x:Key="App.Text.Header" Color="#333333"/>

    <SolidColorBrush x:Key="Control.Background.Default" Color="White"/>
    <SolidColorBrush x:Key="Control.Border.Default" Color="#B0B0B0"/>

    <SolidColorBrush x:Key="Button.Background.Default" Color="#E0E0E0" />
    <SolidColorBrush x:Key="Button.Background.MouseOver" Color="#E8E8E8" />
    <SolidColorBrush x:Key="Button.Background.Pressed" Color="#D0D0D0" />

    <SolidColorBrush x:Key="Tab.Background.Default" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="Tab.Background.MouseOver" Color="#E5E5E5"/>
    <SolidColorBrush x:Key="Tab.Background.Selected" Color="White"/>

    <SolidColorBrush x:Key="Separator.Background.Default" Color="#D0D0D0" />
    <SolidColorBrush x:Key="GroupBox.Border.Default" Color="#D0D0D0"/>

    <SolidColorBrush x:Key="Header.Background.Default" Color="#F0F0F0"/>
    <SolidColorBrush x:Key="Header.Border.Default" Color="#B0B0B0"/>

    <SolidColorBrush x:Key="ListItem.Background.Undefined" Color="#F5F5F5"/>
    <SolidColorBrush x:Key="ListItem.Text.Undefined" Color="Black"/>
    <SolidColorBrush x:Key="ListItem.Background.MouseOver" Color="#E3F2FD"/>
    <SolidColorBrush x:Key="ListItem.Background.Selected" Color="#BBDEFB"/>
    <SolidColorBrush x:Key="ListItem.Text.Selected" Color="Black"/>

    <SolidColorBrush x:Key="TitleBar.Background.Default" Color="#f0f0f0"/>
    <SolidColorBrush x:Key="CaptionButton.Background.MouseOver" Color="#E0E0E0"/>
    <SolidColorBrush x:Key="Search.Placeholder.Foreground" Color="Gray"/>
</ResourceDictionary>